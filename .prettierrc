{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "configPath": "",
  "disableLanguages": [],
  "documentSelectors": [],
  "embeddedLanguageFormatting": "auto",
  "enable": true,
  "endOfLine": "crlf",
  "htmlWhitespaceSensitivity": "css",
  "ignorePath": ".prettierignore",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "packageManager": "yarn",
  "prettierPath": "",
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requireConfig": false,
  "requirePragma": false,
  "resolveGlobalModules": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "useEditorConfig": true,
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "withNodeModules": false,
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
